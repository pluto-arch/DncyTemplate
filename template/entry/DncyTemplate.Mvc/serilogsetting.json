{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Enrichers.Sensitive", "Serilog.Sinks.Async" ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      {
        "Name": "WithSensitiveDataMasking",
        "Args": {
          "options": {
            "MaskValue": "**??**",
            "MaskProperties": [
              "email",
              "password",
              "token",
              "access_token"
            ],
            "Mode": "Globally"
          }
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Error",
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "==> {Timestamp:yy/MM/dd HH:mm:ss.fff} [{Level:u3}]{Message:j}{NewLine}{Properties:j}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ],
    "Properties": {
      "AppName": "DncyTemplate.Api"
    }
  }
}